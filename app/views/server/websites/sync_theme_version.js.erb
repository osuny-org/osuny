var version = document.querySelector('#website-<%= @website.id %> .js-version');
version.innerHTML = "<%= j @website.theme_version if @website.theme_version_url.present? %>";
var notyf = new Notyf();
notyf.open({
    type: 'success',
    position: {
        x: 'center',
        y: 'bottom'
    },
    message: "<%= j(t('server_admin.websites.sync_theme_version_notice', website: @website.to_s_in(current_language))) %>",
    duration: 5000,
    ripple: true,
    dismissible: true
});
