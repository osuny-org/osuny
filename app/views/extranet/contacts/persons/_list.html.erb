<% if people.any? %>
  <% people_paged = people.ordered.page(params[:page]).per(60) %>
  <div class="row">
    <% people_paged.each do |person| %>
      <div class="col-md-2">
        <%= render 'extranet/contacts/persons/person', person: person %>
      </div>
    <% end %>
  </div>
  <%= paginate people_paged %>
