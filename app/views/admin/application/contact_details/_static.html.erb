<% 
depth ||= 0
indentation = '  ' * depth
l10n ||= nil
language ||= l10n&.language
parts = [
  :postal_address,
  :phone_numbers,
  :emails,
  :websites,
  :social_networks
]
%>
<%= indentation %>contact_details:
<% parts.each do |part| %>
<%= render "admin/application/contact_details/static/#{part}", 
            about: about,
            l10n: l10n,
            language: language,
            depth: depth+1 %>
<% end %>